    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.0.2",
    "babel-loader": "^10.0.0",
    "clsx": "^2.1.1",
    "daisyui": "^5.0.43",
    "eslint-plugin-mdx": "^3.5.0",
    "jest": "^30.0.3",
    "microbundle": "^0.15.1",
    "plop": "^4.0.1",
    "prettier": "^3.6.2",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "storybook": "^9.0.13",
    "styled-jsx": "^5.1.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.1.1",
    "typescript": "^5.8.3",
    "vite": "^5.4.19"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11"
  }
